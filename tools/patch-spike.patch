index 0000000..9ba2e92
--- /dev/null
+++ b/riscv/insns/saes32_decs.h
@@ -0,0 +1,14 @@
+
+#include "saes_common.h"
+
+uint8_t     bs = insn.bs();
+
+uint8_t     t0 = RS2 >> (8*bs);
+uint8_t      x = AES_DEC_SBOX[t0];
+uint32_t     u = x;
+
+u = (u << (8*bs)) | (u >> (32-8*bs));
+
+WRITE_RD(u ^ RS1);
+
+
diff --git a/riscv/insns/saes32_decsm.h b/riscv/insns/saes32_decsm.h
