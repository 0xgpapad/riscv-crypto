+++ b/riscv/insns/saes32_decsm.h
@@ -0,0 +1,21 @@
+
+#include "saes_common.h"
+
+uint8_t     bs = insn.bs();
+
+uint8_t     t0 = RS2 >> (8*bs);
+uint8_t      x = AES_DEC_SBOX[t0];
+uint32_t     u ;
+
+u = (AES_GFMUL(x,0xb) << 24) |
+    (AES_GFMUL(x,0xd) << 16) |
+    (AES_GFMUL(x,0x9) <<  8) |
+    (AES_GFMUL(x,0xe) <<  0) ;
+
+u = (u << (8*bs)) | (u >> (32-8*bs));
+
+WRITE_RD(u ^ RS1);
+
+
+
+
